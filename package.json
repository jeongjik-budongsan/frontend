{
  "name": "jeongjik-budongsan-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "pages:dev": "wrangler pages dev --compatibility-date=2023-11-21 --proxy 5173 -- npm run dev",
    "pages:deploy": "npm run build && wrangler pages deploy .svelte-kit/cloudflare",
    "schema": "npx openapi-typescript http://127.0.0.1:8000/openapi.json -o ./src/types/schema.d.ts",
    "dbgen": "npx supabase gen types typescript --project-id hxaymmlsctexxndrgdmr > src/supabase/database.types.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-cloudflare": "^2.3.3",
    "@sveltejs/kit": "^1.27.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.30.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-geolocation": "^0.3.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "wrangler": "^3.18.0"
  },
  "type": "module",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "proj4": "^2.9.2"
  }
}
